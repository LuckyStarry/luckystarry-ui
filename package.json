{
  "name": "luckystarry-ui",
  "version": "1.0.5",
  "description": "这是一个管理后台的框架组件，封装了常用的后台功能，为使用者提供快速统一的后台搭建解决方案。",
  "main": "lib/index.js",
  "scripts": {
    "clean": "del-cli lib -f",
    "lint": "tslint -p . -c tslint.json 'src/**/*.ts'",
    "prebuild": "npm run clean && npm run svg && npm run lint",
    "build": "webpack --mode production --config ./build/webpack.config.js",
    "dev": "webpack-dev-server --mode development --config ./build/webpack.dev.config.js",
    "pretest": "del-cli .nyc_output coverage -f",
    "pretest:coveralls": "npm run pretest",
    "test:coveralls": "cross-env BABEL_ENV=test karma start karma.conf.coveralls.js --single-run",
    "test": "cross-env BABEL_ENV=test karma start karma.conf.js --single-run",
    "svg": "vsvg -s ./src/icons/svg -t ./src/icons/components --ext ts --es6",
    "prepack": "npm run build",
    "x": "npm i --registry https://registry.npm.taobao.org"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuckyStarry/luckystarry-ui.git"
  },
  "author": "SUN BO <starry@vip.qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LuckyStarry/luckystarry-ui/issues"
  },
  "homepage": "https://github.com/LuckyStarry/luckystarry-ui#readme",
  "dependencies": {
    "@types/sortablejs": "^1.10.4",
    "axios": "^0.21.1",
    "element-ui": "^2.15.0",
    "fuse.js": "^6.0.0",
    "js-cookie": "^2.2.1",
    "luckystarry-collections": "^1.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.1.0",
    "screenfull": "^5.0.2",
    "sortablejs": "^1.10.2",
    "uuid": "^8.0.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-i18n": "^8.17.7",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.1.6",
    "vue-svgicon": "^3.2.6",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@types/chai": "^4.2.11",
    "@types/js-cookie": "^2.2.6",
    "@types/mocha": "^7.0.2",
    "@types/nprogress": "^0.2.0",
    "@types/uuid": "^8.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "del-cli": "^3.0.1",
    "file-loader": "^6.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.2",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-coveralls": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.2.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "ts-loader": "^7.0.4",
    "tslint": "^6.1.2",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^5.0.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
